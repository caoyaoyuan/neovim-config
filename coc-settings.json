{
  "semanticTokens.enable": true,
  "outline.autoPreview": false,
  "list.source.files.defaultOptions": ["--number-select"],
  "suggest.formatItems": ["kind","abbr","menu", "shortcut"],//["abbr", "menu", "kind", "shortcut"],
  "suggest.defaultSortMethod": "length",
  "suggest.autoTrigger": "always",
  "outline.autoWidth": true,
  "outline.previewMaxWidth": 50,
  "diagnostic.refreshOnInsertMode": true,

  "explorer.icon.source": "vim-devicons",
  "explorer.icon.enableNerdfont": true,
  //"explorer.position": "floating",

  "inlayHint.enable": false,
  "colors.filetypes": ["*"],
  "python.pythonPath": "/home/joe/Anaconda/envs/yolov5/bin/python",
  "diagnostic.warningSign": "󰔶",
  "diagnostic.errorSign": "",
  //"diagnostic.warningSign": "\ue00a",
  //"diagnostic.errorSign": "\ue009",
  "translator.proxy": "socks://127.0.0.1:1080",
  "translator.engines": ["bing", "youdao"],
  "suggest.noselect": false,
  //"suggest.enablePreselect": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualText": false,
  "suggest.virtualText": false,
  "cmake.lsp.enable": true,
  "list.source.files.args": ["--hidden", "--files"],
  "clangd.fallbackFlags": ["-I${workspaceForlder}"],
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"],
      "args": ["serve", "-debug", "0.0.0.0:8484", "-rpc.trace"]
    },
    "cmake": {
      "command": "cmake-language-server",
      "filetypes": ["cmake"],
      "rootPatterns": ["build/"],
      "initializationOptions": {
        "buildDirectory": "build"
      }
    }
  },
  "clangd.arguments": [],
  "snippets.extends": {
    "cpp": ["c"],
    "javascript": ["javascript"],
    "typescript": ["javascript"]
  },
  "git.changedDecorator": "*",
  "git.branchCharacter": "",
  "git.conflictedDecorator": "x",
  "git.stagedDecorator": "●",
  "git.untrackedDecorator": "…",
  "git.changedSign.text": "~",
  "git.addedSign.text": "",
  "git.removedSign.text": "_",
  "git.topRemovedSign.text": "‾",
  "git.changeRemovedSign.text": "≃",
  "coc.preferences.formatOnSaveFiletypes": [".py", ".json"],
  "suggest.completionItemKindLabels": {
    "function": "󰊕",
    "method": "",
    "variable": "󱄑",
    "constant": "",
    "struct": "פּ",
    "class": "",
    "interface": "",
    "text": "",
    "enum": "",
    "enumMember": "",
    "module": "󰕳",
    "color": "",
    "property": "襁",
    "field": "󰐾",
    "unit": "",
    "file": "",
    "value": "",
    "event": "鬒",
    "folder": "",
    "keyword": "",
    "snippet": "",
    "operator": "洛",
    "reference": "",
    "typeParameter": "",
    "namespace": "",
    "default": ""
  },
  "coc.preferences.extensionUpdateCheck": "daily"
}
