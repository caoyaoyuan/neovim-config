{
  "semanticTokens.enable": true,
  "outline.autoPreview": false,
  "list.source.files.defaultOptions": ["--number-select"],
  "suggest.formatItems": ["abbr", "menu", "kind", "shortcut"],
  "suggest.defaultSortMethod": "length",
  "suggest.autoTrigger": "always",
  "outline.autoWidth": false,
  "outline.previewMaxWidth": 30,
  "outline.detailAsDescription": false,
  "diagnostic.refreshOnInsertMode": true,
  "explorer.file.child.template": "[selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]",
  "explorer.icon.source": "vim-devicons",
  "explorer.icon.enableNerdfont": true,
  "inlayHint.enable": false,
  "colors.filetypes": ["*"],
  //"highlight.colors.enable": false,
  "python.pythonPath": "/home/joe/Anaconda/bin/python",
  "pyright.disableDiagnostics": true,
  "diagnostic.warningSign": "󰔶",
  "diagnostic.errorSign": "",
  "translator.proxy": "socks://127.0.0.1:1080",
  "translator.engines": ["bing", "youdao"],
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualText": false,
  "suggest.virtualText": false,
  "cmake.lsp.enable": true,
  "list.source.files.args": ["--hidden", "--files"],
  "clangd.fallbackFlags": ["-I${workspaceForlder}", "-rdynamic"],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "linksInHover": false,
        "hoverKind": "SynopsisDocumentation"
        //        "usePlaceholders": true
      }
    },
    "cmake": {
      "command": "cmake-language-server",
      "filetypes": ["cmake"],
      "rootPatterns": ["build/"],
      "initializationOptions": {
        "buildDirectory": "build"
      }
    }
  },
  "clangd.arguments": [],
  "snippets.extends": {
    "cpp": ["c"],
    "javascript": ["javascript"],
    "typescript": ["javascript"]
  },
  "git.changedDecorator": "*",
  "git.branchCharacter": "",
  "git.conflictedDecorator": "x",
  "git.stagedDecorator": "●",
  "git.untrackedDecorator": "…",
  "git.changedSign.text": "~",
  "git.addedSign.text": "",
  "git.removedSign.text": "_",
  "git.topRemovedSign.text": "‾",
  "git.changeRemovedSign.text": "≃",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "go",
    "python",
    "json",
    "lua"
  ],
  "suggest.completionItemKindLabels": {
    "function": "󰊕",
    "method": "",
    "variable": "󱄑",
    "constant": "",
    "struct": "פּ",
    "class": "",
    "interface": "",
    "text": "",
    "enum": "",
    "enumMember": "",
    "module": "󰕳",
    "color": "",
    "property": "襁",
    "field": "󰐾",
    "unit": "",
    "file": "",
    "value": "",
    "event": "鬒",
    "folder": "",
    "keyword": "",
    "snippet": "",
    "operator": "洛",
    "reference": "",
    "typeParameter": "",
    "namespace": "",
    "default": ""
  },
  "coc.preferences.extensionUpdateCheck": "daily"
}
